{
  "name": "sybau-picture",
  "version": "1.0.0",
  "description": "AI-powered Sybau Lazer Dim 700 style meme generator",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run protect:check && next build",
    "build:unsafe": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "node scripts/eslint-cleanup.js",
    "setup": "node scripts/setup-database.js",
    "setup:db": "npx prisma generate && npx prisma db push && npx prisma db seed",
    "setup:full": "npm run setup && npm run setup:db",
    "health": "node scripts/health-check.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma db push --force-reset && npx prisma db seed",
    "clean": "npm run clean:test && npm run clean:build",
    "clean:test": "rm -rf app/api/test-* app/api/debug-* app/api/diagnose app/api/network-test",
    "clean:build": "rm -rf .next out dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "deploy:vercel": "vercel --prod",
    "protect:init": "node protection/scripts/protection-manager.js init",
    "protect:check": "node protection/scripts/protection-manager.js check",
    "protect:build": "node protection/scripts/protection-manager.js build",
    "protect:status": "node protection/scripts/protection-manager.js status",
    "protect:backup": "node protection/scripts/backup-system.js backup",
    "protect:restore": "node protection/scripts/backup-system.js restore",
    "protect:integrity": "node protection/scripts/file-integrity-check.js verify",
    "protect:save-checksums": "node protection/scripts/file-integrity-check.js save",
    "protect:pre-build": "node protection/scripts/pre-build-check.js",
    "protect:maintenance": "node protection/scripts/protection-manager.js maintenance",
    "protect:enable": "node protection/scripts/protection-manager.js enable",
    "protect:disable": "node protection/scripts/protection-manager.js disable",
    "dev:utf8": "dev-utf8.bat",
    "encoding:fix": "node protection/scripts/encoding-fix.js",
    "encoding:check": "node protection/scripts/encoding-fix.js check"
  },
  "dependencies": {
    "@fal-ai/serverless-client": "^0.15.0",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.2.14",
    "@sentry/nextjs": "^7.87.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@uploadthing/react": "^6.2.2",
    "@upstash/ratelimit": "^1.0.0",
    "@upstash/redis": "^1.25.1",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "postcss": "^8.4.32",
    "posthog-js": "^1.96.1",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "replicate": "^0.25.2",
    "sharp": "^0.33.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "uploadthing": "^6.2.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "^15.3.4",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "supertest": "^6.3.3",
    "tsx": "^4.20.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
